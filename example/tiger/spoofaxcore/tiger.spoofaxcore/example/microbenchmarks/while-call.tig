let
  function fact(n : int) : int = 
      if n < 1 then 1 else (n * fact(n - 1))
  
  function bar() : int = 42
  
  function foo() : string = ""
  
  function plus() : int = 1 + 2
  
  function minus() = flush()
  
in
  for i := 0 to 10000
  do
    (
    fact(10);
    bar();
    foo();
    plus();
    minus()
    )
end