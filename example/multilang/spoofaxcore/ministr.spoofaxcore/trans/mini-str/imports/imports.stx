module mini-str/imports/imports

imports
  modules/modules
  signatures/mini-str/mini-str-sig

  abstract-sig/conflicts/sorts
  abstract-sig/conflicts/constructors

  mini-str/imports/rules

rules

  strImportsOk maps strImportOk(*, list(*))

  strImportOk : scope * MOD

  strImportOk(s, n) :- {s_mod}
    import(s, n, _) == s_mod,
    rulesImpOk(n, s, s_mod).

